<center>
<h1>Log in</h1>


<%= bootstrap_form_for(:session, url: login_path, html: { class: "w-25"})  do |f| %>
   
    <%= f.text_field :username%><br>
   
    <%= f.password_field :password %><br><br>
    

    <%= f.submit "Log in"%><br>
    
<% end %>

    <p><%= button_to "Log in with Google", '/auth/google_oauth2' %></p>
    <p><%= button_to "Log in with Facebook", '/auth/facebook'  %></p>



<p> New reader? <%= link_to "Sign up", signup_path %>
</center>